{
  "name": "@solid-devtools/transform",
  "version": "0.10.1",
  "description": "Babel plugin for transforming SolidJS code in development to enchance solid-devtools usage.",
  "license": "MIT",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "contributors": [
    "Erik Demaine <edemaine@mit.edu>"
  ],
  "homepage": "https://github.com/thetarnav/solid-devtools/tree/main/packages/transform#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetarnav/solid-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/thetarnav/solid-devtools/issues"
  },
  "keywords": [
    "solid",
    "devtools",
    "components",
    "babel",
    "plugin",
    "jsx"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/generator": "^7.19.6",
    "@babel/parser": "^7.19.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/traverse": "^7.19.6",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.26.0"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-syntax-typescript": "^7.18.6",
    "@babel/types": "^7.19.4",
    "@solid-devtools/shared": "workspace:^0.10.4",
    "@solid-devtools/debugger": "workspace:^0.17.2"
  },
  "peerDependencies": {
    "solid-js": "^1.6.2",
    "vite": "^2.2.3 || ^3.0.0 || ^4.0.0"
  }
}
