{
  "name": "@solid-devtools/transform",
  "version": "0.7.5",
  "description": "Babel plugin for transforming SolidJS code in development to enchance solid-devtools usage.",
  "license": "MIT",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "contributors": [
    "Erik Demaine <edemaine@mit.edu>"
  ],
  "homepage": "https://github.com/thetarnav/solid-devtools/tree/main/packages/transform#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetarnav/solid-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/thetarnav/solid-devtools/issues"
  },
  "keywords": [
    "solid",
    "devtools",
    "components",
    "babel",
    "plugin",
    "jsx"
  ],
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/generator": "^7.19.3",
    "@babel/parser": "^7.19.3",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/traverse": "^7.19.3",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-typescript": "^7.18.6",
    "@babel/types": "^7.19.3",
    "@solid-devtools/shared": "workspace:^0.9.0",
    "solid-js": "^1.5.7"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0",
    "vite": "^2.2.3 || ^3.0.0"
  }
}
