{
  "name": "solid-devtools",
  "version": "0.26.0",
  "description": "Runtime library for hooking up SolidJS application with Solid Devtools Extension",
  "license": "MIT",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "contributors": [],
  "homepage": "https://github.com/thetarnav/solid-devtools/tree/main/packages/main#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetarnav/solid-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/thetarnav/solid-devtools/issues"
  },
  "keywords": [
    "solid",
    "devtools",
    "reactivity",
    "extension",
    "chrome"
  ],
  "private": false,
  "sideEffects": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index/index.js",
  "module": "./dist/index/index.js",
  "types": "./dist/index/index.d.ts",
  "browser": {},
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index/index.d.ts",
        "default": "./dist/index/index.js"
      }
    },
    "./setup": {
      "import": {
        "types": "./dist/setup/index.d.ts",
        "default": "./dist/setup/index.js"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite/index.d.ts",
        "default": "./dist/vite/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "setup": [
        "./dist/setup/index.d.ts"
      ],
      "vite": [
        "./dist/vite/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit --paths null"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/generator": "^7.21.5",
    "@babel/parser": "^7.21.8",
    "@babel/plugin-syntax-jsx": "^7.21.4",
    "@babel/traverse": "^7.21.5",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.5",
    "solid-start": "^0.2.9"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-syntax-typescript": "^7.21.4",
    "@babel/types": "^7.21.5",
    "@solid-devtools/debugger": "workspace:^",
    "@solid-devtools/shared": "workspace:^",
    "@solid-primitives/event-bus": "^1.0.7",
    "@solid-primitives/utils": "^6.2.0",
    "solid-js": "^1.7.5",
    "type-fest": "^3.11.0",
    "vite": "^4.3.8"
  },
  "peerDependencies": {
    "solid-js": "^1.7.0",
    "vite": "^2.2.3 || ^3.0.0 || ^4.0.0"
  },
  "optionalDependencies": {
    "solid-start": "^0.2.26"
  },
  "packageManager": "pnpm@8.2.0"
}
