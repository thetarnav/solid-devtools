{
  "name": "solid-devtools",
  "version": "0.26.0",
  "description": "Runtime library for hooking up SolidJS application with Solid Devtools Extension",
  "license": "MIT",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "contributors": [],
  "homepage": "https://github.com/thetarnav/solid-devtools/tree/main/packages/main#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetarnav/solid-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/thetarnav/solid-devtools/issues"
  },
  "keywords": [
    "solid",
    "devtools",
    "reactivity",
    "extension",
    "chrome"
  ],
  "private": false,
  "sideEffects": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/setup/index.js",
  "module": "./dist/setup/index.js",
  "types": "./dist/setup/index.d.ts",
  "browser": {},
  "exports": {
    "./setup": {
      "import": {
        "types": "./dist/setup/index.d.ts",
        "default": "./dist/setup/index.js"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite/index.d.ts",
        "default": "./dist/vite/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "setup": [
        "./dist/setup/index.d.ts"
      ],
      "vite": [
        "./dist/vite/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit --paths null"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/generator": "^7.20.14",
    "@babel/parser": "^7.20.15",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/traverse": "^7.20.13",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.2",
    "solid-start": "^0.2.9"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-typescript": "^7.18.6",
    "@babel/types": "^7.20.7",
    "@solid-devtools/debugger": "workspace:^0.21.0",
    "@solid-devtools/shared": "workspace:^0.11.0",
    "@solid-primitives/event-bus": "^1.0.0",
    "@solid-primitives/utils": "^5.1.0",
    "type-fest": "^3.5.7"
  },
  "peerDependencies": {
    "solid-js": "^1.6.11",
    "vite": "^2.2.3 || ^3.0.0 || ^4.0.0"
  },
  "optionalDependencies": {
    "solid-start": "^0.2.20"
  },
  "packageManager": "pnpm@7.22.0"
}
